to safe :msg
  print "BEFORE\ CATCH
  catch "test [display :msg]
  print "AFTER\ CATCH time
  if errorp [show error]
  forever [type ". if repcount >= 10 [stop]]
  for [:a :global 5 * 2 -0.3] [print "+]
  print "
end safe

to display :msg
  repeat 10 - 1 [type  word "- repcount]
  print "-
  print :msg "MORE sqrt -21 + square (thing :redirect) * 2 [Info] first [a b c]
  iftrue [stop]
  repeat 2 * 2 [type "-]
  print "-
  bye 5
end display

to square :x
  if :x < 0 [throw "test]
  if :x = 0 [output 0]
  op :x * :x
end "square
